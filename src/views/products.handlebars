<div id="container">
    <h1>Lista de productos</h1>
    {{#each productos}}

<h2>{{this.title}}</h2>
<p id="idp">id:{{this.id}}</p>
<p>descripcion:{{this.description}}</p>
<p>precio:{{this.price}}</p>
<p>categoria: {{this.category}}</p>
<p>stock: {{this.stock}}</p>
<button id="agregar">Agregar</button>
{{/each}}


</div>
